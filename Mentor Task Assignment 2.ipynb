    " \n",
    "def compute_dist(my_list):\n",
    "     \n",
    "    f = {}\n",
    "    for items in my_list:\n",
    "        f[items] = my_list.count(items)\n",
    "     \n",
    "    for key, value in f.items():\n",
    "        print (str(key)+':'+str(value))\n",
    "\n",
    "n=list(input().split())\n",
    "\n",
    "compute_dist(n)"
   ]
